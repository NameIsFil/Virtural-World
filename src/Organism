class Organism {
    initiatve;
    
    constructor() {

    }

    action() {

    }
}

class Animal extends Organism {
    fight() {

    }
    mate() {

    }
}

class Player extends Animal {
    playerElement;
    randomYIndex = Math.floor(Math.random() * 20);
    randomXIndex = Math.floor(Math.random() * 20);
    
    constructor() {

    }

    spawnPlayer() {
        this.playerElement = this.gameGrid[this.randomYIndex][this.randomXIndex];
        this.playerElement.divElement.classList.add('player-tile');
      }
    

}

export { Organism, Animal, Player };